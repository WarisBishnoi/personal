{
	"agent": {
		"metrics_collection_interval": 30,
		"run_as_user": "cwagent"
	},
	"metrics": {
		"metrics_collected": {
			"cpu": {
				"measurement": [
					"cpu_usage_idle",
					"cpu_usage_iowait",
					"cpu_usage_user",
					"cpu_usage_system"
				],
				"metrics_collection_interval": 30,
				"totalcpu": false
			},
			"disk": {
				"measurement": [
					"used_percent",
					"inodes_free"
				],
				"metrics_collection_interval": 30,
				"resources": [
					"*"
				]
			},

			"mem": {
				"measurement": [
					"mem_used_percent",
			                "mem_total"
                               ],
				"metrics_collection_interval": 30
			}
                }

	}
}
